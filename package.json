{
	"name": "blissbase",
	"private": true,
	"version": "0.0.4",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "npm run test:run:ci && vite build",
		"test:run:ci": "if [ \"$VERCEL\" = \"1\" ]; then echo 'Skipping events.test.ts in Vercel environment due to PGlite array serialization issue'; vitest run --exclude='**/events.test.ts'; else npm run test:run; fi",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:debug": "playwright test --debug",
		"test:all": "bun run test:run && bun run test:e2e",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:pull": "drizzle-kit pull",
		"scrape": "bun run scripts/scrape-websites.ts",
		"scrapeImport": "bun run scripts/import-events.ts",
		"deduplicateImages": "bun run scripts/deduplicate-images.ts",
		"cleanupUnusedImages": "bun run scripts/cleanup-unused-images.ts",
		"cleanupUnusedImages:dry": "bun run scripts/cleanup-unused-images.ts --dry-run",
		"deploy:cf": "npx wrangler deploy --cwd blissbase-telegram-entry",
		"wuchale:clean": "wuchale --clean"
	},
	"devDependencies": {
		"@ai-sdk/openai": "^3.0.30",
		"@electric-sql/pglite": "^0.3.15",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.18.0",
		"@iconify/json": "^2.2.441",
		"@iconify/tailwind4": "^1.2.1",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-vercel": "^5.10.3",
		"@sveltejs/enhanced-img": "^0.9.3",
		"@sveltejs/kit": "^2.52.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.2.0",
		"@types/bun": "^1.3.9",
		"@types/cheerio": "^0.22.35",
		"@types/node": "^22.19.11",
		"@types/sharp": "^0.32.0",
		"@types/ws": "^8.18.1",
		"@vercel/functions": "^2.2.13",
		"@vite-pwa/sveltekit": "^1.1.0",
		"ai": "^6.0.94",
		"bits-ui": "^2.16.1",
		"calendar-link": "^2.11.0",
		"cheerio": "^1.2.0",
		"daisyui": "^5.5.18",
		"dotenv": "^16.6.1",
		"drizzle-kit": "^0.31.9",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^16.5.0",
		"postgres": "^3.4.8",
		"posthog-node": "^5.24.17",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup": "^4.57.1",
		"sharp": "^0.34.5",
		"sharp-phash": "^2.2.0",
		"svelte": "^5.53.0",
		"svelte-check": "^4.4.1",
		"svelte-meta-tags": "^4.5.0",
		"tailwindcss": "^4.0.0",
		"telegraf": "^4.16.3",
		"telegram": "^2.26.22",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"unplugin-icons": "^22.5.0",
		"valibot": "^1.2.0",
		"vite": "^7.3.1",
		"vite-plugin-pwa": "^1.2.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@bradenmacdonald/s3-lite-client": "npm:@jsr/bradenmacdonald__s3-lite-client@^0.9.5",
		"@editorjs/editorjs": "^2.31.3",
		"@editorjs/header": "^2.8.8",
		"@editorjs/list": "^2.0.9",
		"@editorjs/marker": "^1.4.0",
		"@internationalized/date": "^3.11.0",
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.97.0",
		"@wuchale/svelte": "^0.17.9",
		"@wuchale/vite-plugin": "^0.15.7",
		"drizzle-orm": "^0.45.1",
		"editorjs-html": "^4.0.5",
		"ky": "^1.14.3",
		"posthog-js": "^1.351.4",
		"ws": "^8.19.0",
		"wuchale": "^0.18.11"
	}
}
