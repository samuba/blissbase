{
	"name": "blissbase",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "npm run test:run && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"db:pull": "drizzle-kit pull",
		"scrape": "bun run scripts/scrape-germany.ts",
		"scrapeImport": "bun run scripts/import-events.ts",
		"deduplicateImages": "bun run scripts/deduplicate-images.ts",
		"cleanupUnusedImages": "bun run scripts/cleanup-unused-images.ts",
		"cleanupUnusedImages:dry": "bun run scripts/cleanup-unused-images.ts --dry-run",
		"deploy:cf": "npx wrangler deploy --cwd blissbase-telegram-entry"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@iconify/json": "^2.2.335",
		"@iconify/tailwind4": "^1.0.6",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.36.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.0",
		"@types/bun": "^1.2.10",
		"@types/cheerio": "^0.22.35",
		"@types/node": "^22",
		"@types/sharp": "^0.32.0",
		"@types/ws": "^8.18.1",
		"@vercel/functions": "^2.2.13",
		"@vite-pwa/sveltekit": "^1.0.0",
		"bits-ui": "^1.8.0",
		"calendar-link": "^2.10.0",
		"cheerio": "^1.0.0",
		"cloudinary": "^2.7.0",
		"daisyui": "^5.0.50",
		"dotenv": "^16.5.0",
		"drizzle-kit": "^0.31.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"postgres": "^3.4.7",
		"posthog-node": "^5.1.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sharp": "^0.34.3",
		"sharp-phash": "^2.2.0",
		"svelte": "^5.38.3",
		"svelte-check": "^4.0.0",
		"svelte-meta-tags": "^4.4.0",
		"tailwindcss": "^4.0.0",
		"telegraf": "^4.16.3",
		"telegram": "^2.26.22",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"unplugin-icons": "^22.1.0",
		"valibot": "^1.1.0",
		"vite": "^6.3.5",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@editorjs/editorjs": "^2.30.8",
		"@editorjs/header": "^2.8.8",
		"@editorjs/list": "^2.0.8",
		"@editorjs/marker": "^1.4.0",
		"@internationalized/date": "^3.8.0",
		"@libsql/client": "^0.14.0",
		"drizzle-orm": "^0.44.5",
		"editorjs-html": "^4.0.5",
		"ky": "^1.8.1",
		"posthog-js": "^1.255.1",
		"ws": "^8.18.1"
	}
}
